{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "description": "oapi-codegen v2.3.0 is the first release after migrating to a community-owned GitHub organisation",
      "matchPackagePatterns": [
        "github.com/deepmap/oapi-codegen"
      ],
      "replacementName": "github.com/oapi-codegen/oapi-codegen/v2",
      "replacementVersion": "v2.3.0"
    }
  ],
  "postUpdateOptions": [
    "gomodTidy",
    "gomodUpdateImportPaths"
  ],
  "prConcurrentLimit": 1000,
  "branchConcurrentLimit": 1000
}
